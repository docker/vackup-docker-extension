{
  "icon": "docker.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "ext.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "Vackup",
      "src": "index.html",
      "root": "ui",
      "backend": {
        "socket": "ext.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/host/darwin-amd64/volumes-share-client"
          }
        ],
        "linux": [
          {
            "path": "/host/linux-amd64/volumes-share-client"
          }
        ],
        "windows": [
          {
            "path": "/host/windows-amd64/volumes-share-client.exe"
          }
        ]
      }
    ]
  }
}
